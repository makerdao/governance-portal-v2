  "scripts": {
    "prepare": "husky install",
    "dev": "next",
    "dev-https": "node server.js",
    "build": "node ./scripts/fetchTags.mjs && yarn build-sdk && next build",
    "start": "next start",
    "export": "next export",
    "test": "jest --config=jest.config.js --runInBand --watch",
    "test:ci": "jest --runInBand --ci --coverage --detectOpenHandles --forceExit",
    "e2e": "APP_ENV=test start-server-and-test dev http://localhost:3000 cypress",
    "e2e:headless": "APP_ENV=test npm run fund && start-server-and-test dev http://localhost:3000 cypress:headless",
    "cypress": "BABEL_ENV=cypress-test cypress open",
    "cypress:headless": "BABEL_ENV=cypress-test cypress run",
    "fund": "npx hardhat run scripts/setup.js --network goerli",
    "hardhat": "npx hardhat node --network hardhat ",
    "hardhat:goerli:config": "yarn hardhat --config hardhat-fork-goerli.config.js",
    "hardhat:arb-testnet:config": "yarn hardhat --config hardhat-fork-arbTestnet.config.js --port 8546",
    "hardhat:gasless:config": "yarn hardhat:goerli:config & yarn hardhat:arb-testnet:config",
    "hardhat:detached": "npm run hardhat & TASKHARDHAT_PID=$! ",
    "start:detached": "sleep 5 && npm run fund && yarn dev && kill $TASKHARDHAT_PID",
    "start:ci": "yarn build-sdk & npm run --silent hardhat:detached & npm run start:detached",
    "prettier": "prettier --config .prettierrc --write pages/ modules/ lib/ cypress/",
    "lint": "eslint --ext .tsx,.ts pages/ modules/ lib/",
    "gql-generate": "graphql-codegen --config codegen.yml",
    "build-sdk": "eth-sdk --path modules/contracts && yarn contracts:compile:abi",
    "contracts:compile:abi": "typechain --target ethers-v5 \"./modules/contracts/ethers/**/*.json\"",
    "combine:coverage": "mkdir -p combined-coverage && cp coverage/coverage-final.json combined-coverage/jest-final.json && cp cypress-coverage/coverage-final.json combined-coverage/cypress-final.json && npx nyc merge combined-coverage combined-coverage/combined.json && npx nyc report --temp-directory combined-coverage --report-dir combined-coverage --reporter=lcov"
  },
